{
  "name": "landslide-css",
  "version": "1.4.0",
  "description": "Superclean, powerful, responsive, Sass-based, BEM-syntax CSS grid system",
  "main": "_landslide.scss",
  "repository": {
    "type": "git",
    "url": "https://github.com/lucamamprin/landslide.git"
  },
  "author": "Tom Hare <tom@hare.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lucamamprin/landslide/issues"
  },
  "scripts": {
    "dev": "sass --watch ./src/_landslide.scss ./build/landslide.css",
    "build": "sass -s compressed --no-source-map ./src/_landslide.scss ./build/landslide.css"
  },
  "homepage": "https://github.com/lucamamprin/landslide",
  "keywords": [
    "css",
    "grid",
    "oocss",
    "bem",
    "flexbox",
    "progressive enhancement"
  ],
  "devDependencies": {
    "sass": "^1.20.3"
  }
}
